{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/(.*)\\.(css|js|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot)",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" }
    },
    {
      "src": "/(.*)\\.(csv|xlsx|xls)",
      "headers": { "Cache-Control": "public, max-age=0" }
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/[^.]+",
      "dest": "/index.html"
    }
  ]
}
 
 